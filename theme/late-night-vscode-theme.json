         "settings": {
            "foreground": "#7799FF"
         }
      },

      {
         "name": "COMMENT",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": { "foreground": "#FFFFFF68" }
      },

      {
         "scope": ["keyword", "storage"],
         "settings": {
            "foreground": "#0099BB"
         }
      },

      {
         "name": "STRING LITERAL & STRING TEMPLATE",
         "scope": [
            "string",
