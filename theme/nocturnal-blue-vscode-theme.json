    /*



   ||========================================================================||
   ||"SAVE THESE PROPERTIES UNTIL THEY ARE STYLED IN THE "tokenColors" ARRAY"||
   ||==|| "punctuation.definition.directive",                  ||========||
   || "punctuation.definition.template-expression",            ||
   || "meta.preprocessor.include punctuation.definition.string"||
   ||==========================================================||

   *---------------------------------------------------------------------------*
                     SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*

   LITERAL BOOLEAN         -> #EEA0FF
   LITERAL NUMERIC         -> #BBAA44
   LITERAL STRINGS         -> #DDAA99

   VARIABLES               -> #90C2F8
   VARIABLE DECLARATIONS   -> #C499D0
   OBJECT-MEMBERS          -> #C499D0

   CLASS                   -> #C490FF
   CLASS-MEMBERS           -> #C499D0

   FUNCTIONS               -> #2CB8DD
   FUNCTION-MEMBERS        -> #2CB8DD
   VARIABLE PARAMETERS     -> #ACC255

   ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/
    "semanticTokenColors": {
        "class": "#C490FF",
        "interface": "#C490FF",
        "struct": "#C490FF",
        "namespace": "#C490FF",
        "function": "#2CB8DD",
        "method": "#2CB8DD",
        "variable": "#90C2F8",
        "property": "#90C2F8",
        "variable.declaration": "#C499D0",
        "property.declaration": "#C499D0",
        "parameter": "#ACC255"
        // "type": "#FFF0",
        // "typeParameter""#FFF0",
    },
    "tokenColors": [
        {
            "name": "COMMENTS",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#34802E"
            }
        },
        {
            "name": "PUNCTUATION",
            "scope": [
                "variable",
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#8899AA",
                "fontStyle": ""
            }
        },
        {
            "name": "VARIABLES",
            "scope": [
