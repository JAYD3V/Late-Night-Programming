            "settings": {
                "foreground": "#44AA88",
                "fontStyle": "italic"
            }
        },
        {
            "name": "VARIABLES: PARAMETERS",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#BBCC80",
                "fontStyle": ""
            }
        },
        {
            "name": "VARIABLES: CLASS-MEMBERS & OBJECT-MEMBERS",
            "scope": [
                "variable.property",
                "variable.object.property",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#33A09A"
            }
        },
        {
            "name": "SUPPORT",
            "scope": [
                "support",
                "meta.import variable.other"
            ],
            "settings": {
                "foreground": "#D89880",
                "fontStyle": ""
            }
        },
        {
            "name": "JS/TS Lexically Scoped THIS keyword",
            "scope": "variable.language.this",
            "settings": {
                "foreground": "#AA88FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "TYPES",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.name.class",
                "entity.name.interface",
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "entity.other.inherited-class",
                "entity.other.attribute",
                "new.expr meta.function-call.ts entity.name.function.ts"
