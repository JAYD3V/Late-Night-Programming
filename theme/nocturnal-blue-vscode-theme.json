    /*



   ||========================================================================||
   ||"SAVE THESE PROPERTIES UNTIL THEY ARE STYLED IN THE "tokenColors" ARRAY"||
   ||==|| "punctuation.definition.directive",                  ||========||
   || "punctuation.definition.template-expression",            ||
   || "meta.preprocessor.include punctuation.definition.string"||
   ||==========================================================||

   *---------------------------------------------------------------------------*
                     SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*
   LITERAL BOOLEAN         -> #EEA0FF
   LITERAL NUMERIC         -> #BBAA44
   LITERAL STRINGS         -> #DDAA99

   VARIABLES               -> #90C2F8
   VARIABLE DECLARATIONS   -> #C499D0
   OBJECT-MEMBERS          -> #C499D0

   CLASS                   -> #AA88FF
   CLASS-MEMBERS           -> #C499D0

   FUNCTION-MEMBERS        -> #20A0FF
   VARIABLE PARAMETERS     -> #BBCC80

   ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/
    "semanticTokenColors": {
        "class": "#AA88FF",
        "interface": "#AA88FF",
        "struct": "#AA88FF",
        "namespace": "#AA88FF",
        "function": "#20A0FF",
        "method": "#20A0FF",
        "variable": "#BDBDBD",
        "property": "#90C2F8",
        "variable.declaration": "#C499D0",
        "property.declaration": "#C499D0",
        "parameter": "#BBCC80"
        // "type": "#FFF0",
        // "typeParameter""#FFF0",
    },
    "tokenColors": [
        {
            "name": "COMMENTS",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#306888"
            }
        },
        {
            "name": "PUNCTUATION",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": ""
            }
        },
        {
            "name": "VARIABLES",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#C2C2C2",
                "fontStyle": ""
            }
        },
        {
            "name": "STRINGS",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#55AAAF",
                "fontStyle": ""
            }
        },
        {
            "name": "CONSTANTS",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#BB70AA",
                "fontStyle": "italic"
            }
        },
        {
            "name": "NUMERIC LITERALS",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#C2DF58",
                "fontStyle": ""
            }
        },
        {
            "name": "KEYWORDS (ALL)",
            "scope": [
                "storage",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#206EEE",
                "fontStyle": "bold"
            }
        },
        {
            "name": "KEYWORDS: CONTROL-FLOW",
            "scope": [
                "keyword",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.expression",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#206EEE",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "OPERATORS, GENERIC",
            "scope": [
                "keyword.operator",
                "punctuation.accessor",
                "punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#DD3388",
                "fontStyle": ""
            }
        },
        {
            "name": "TYPES: EXPLICIT TYPES IN TYPE DECLARATIONS",
