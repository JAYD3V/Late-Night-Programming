   /*



   ||========================================================================||
   ||"SAVE THESE PROPERTIES UNTIL THEY ARE STYLED IN THE "tokenColors" ARRAY"||
   ||==|| "punctuation.definition.directive",                      ||========||
   || "punctuation.definition.template-expression",            ||
   || "meta.preprocessor.include punctuation.definition.string"||
   ||==========================================================||

   *---------------------------------------------------------------------------*
                     SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*

   LITERAL BOOLEAN         -> #EEA0FF
   LITERAL NUMERIC         -> #BBAA44
   LITERAL STRINGS         -> #DDAA99

   VARIABLES               -> #90C2F8
   VARIABLE DECLARATIONS   -> #1EBCC4
   OBJECT-MEMBERS          -> #1EBCC4

   CLASS                   -> #C490FF
   CLASS-MEMBERS           -> #1EBCC4

   FUNCTIONS               -> #20ACFF
   FUNCTION-MEMBERS        -> #20ACFF
   VARIABLE PARAMETERS     -> #DDA022

   ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/
   "semanticTokenColors": {
      "class": "#C490FF",
      "interface": "#C490FF",
      "struct": "#C490FF",
      "namespace": "#C490FF",
      "function": "#20ACFF",
      "method": "#20ACFF",
      "variable": "#90C2F8",
      "property": "#90C2F8",
      "variable.declaration": "#1EBCC4",
      "property.declaration": "#1EBCC4",
      "parameter": "#DDA022"
      // "type": "#FFF0",
      // "typeParameter""#FFF0",
   },
   "tokenColors": [
      {
         "name": "COMMENTS",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "foreground": "#34802E"
         }
      },
      {
         "name": "VARIABLES, BRACES, PUNCTUATION & OPERATORS",
         "scope": [
            "variable",
            "punctuation",
            "keyword.operator.assignment",
            "storage.type.function.arrow",
            "meta.brace",
            "keyword.operator.type.annotation"
         ],
         "settings": {
            "foreground": "#C8C8C8",
            "fontStyle": ""
         }
      },
      {
         "name": "OPERATORS",
         "scope": [
            "keyword.operator",
            "punctuation.accessor",
            "punctuation.definition.template-expression"],
         "settings": {
            "foreground": "#A0E880"
         }
      },
      {
         "name": "STRING LITERAL",
         "scope": ["string", "punctuation.definition.string"],
         "settings": {
            "foreground": "#A0BB90",
            "fontStyle": ""
         } // PREV-CLR: "#489028"
      },
      {
         "name": "BOOLEAN LITERAL, UNDEFINED LITERAL, NULL LITERAL, NAN LITERAL, & UTF/ANSI Esc",
         "scope": ["constant"],
         "settings": {
            "foreground": "#44AA22"
         }
      },
      {
         "name": "NUMERIC LITERAL",
         "scope": ["constant.numeric"],
         "settings": {
            "foreground": "#E85030",
            "fontStyle": ""
         }
      },
      {
         "name": "KEYWORDS / STORAGE",
         "scope": [ "keyword", "storage" ],
         "settings": {
            "foreground": "#8874FA",
            "fontStyle": ""
         }
      },
      {
         "name": "KEYWORDS: CONTROL-FLOW, CONDITIONALS, MODIFIERS, etc",
         "scope": [
            "keyword.operator.new",
            "keyword.operator.expression",
            "storage.modifier",
         ],
         "settings": {
            "foreground": "#8874FA",
            "fontStyle": "italic"
         }
      },
      {
         "name": "VARIABLES: LANGUAGE DEFINED",
         "scope": ["variable.language", "support.variable"],
         "settings": { "foreground": "#1EBCC4" }
      },
      {
         "name": "VARIABLES: THIS (lexically-scoped object)",
         "scope": ["variable.language.this"],
         "settings": {
            "foreground": "#1EBCC4",
            "fontStyle": "italic"
         }
      },
      {
         "name": "VARIABLES: PARAMETERS",
         "scope": ["variable.parameter"],
         "settings": {
            "foreground": "#E8A424"
         }
      },
      {
         "name": "VARIABLES: CLASS-MEMBERS & OBJECT-MEMBERS",
         "scope": [
            "variable.object.property",
            "meta.object.member",
            "meta.field.declaration",
            "meta.definition variable",
            "support.variable.property"
         ],
         "settings": {
            "foreground": "#1EBCC4"
         }
      },
      {
         "name": "CLASSES",
         "scope": [
            "support.class",
            "entity.name.class",
            "new.expr meta.function-call.ts entity.name.function.ts",
            "entity.other.inherited-class",
         ],
         "settings": {
            "foreground": "#30B555",
            "fontStyle": "bold"
         }
      },
      {
         "name": "TYPES",
         "scope": [
            "support.type",
            "entity.name.type",
            "entity.name.interface",
            "entity.name.namespace",
            "entity.other.attribute",
            "entity.name.scope-resolution"
         ],
         "settings": {
            "foreground": "#30B555"
         }
      },
      {
         "name": "FUNCTION: FUNCTION/METHOD DEFINITIONS & CALLS",
