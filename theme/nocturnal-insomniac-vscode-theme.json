   *****************************************************************************/
    "semanticTokenColors": {
        "class": "#AA88FF",
        "interface": "#AA88FF",
        "struct": "#AA88FF",
        "namespace": "#AA88FF",
        "function": "#30A0FF",
        "method": "#30A0FF",
