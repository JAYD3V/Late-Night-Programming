        "property.declaration": "#C499D0",
        "parameter": "#BBCC80"
    },
    "tokenColors": [
        {
            "name": "COMMENTS",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#306888"
            }
        },
        {
            "name": "PUNCTUATION",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": ""
            }
