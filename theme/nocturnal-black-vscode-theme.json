   ||"SAVE THESE PROPERTIES UNTIL THEY ARE STYLED IN THE "tokenColors" ARRAY"||
   ||==|| "punctuation.definition.directive",                      ||========||
   || "punctuation.definition.template-expression",            ||
   || "meta.preprocessor.include punctuation.definition.string"||
   ||==========================================================||

   *---------------------------------------------------------------------------*
                     SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*

   LITERAL BOOLEAN         -> #EEA0FF
   LITERAL NUMERIC         -> #BBAA44
   LITERAL STRINGS         -> #DDAA99

   VARIABLES               -> #90C2F8
   VARIABLE DECLARATIONS   -> #1EBCC4
   OBJECT-MEMBERS          -> #1EBCC4

   CLASS                   -> #C490FF
   CLASS-MEMBERS           -> #1EBCC4

   FUNCTIONS               -> #0C94FF
   FUNCTION-MEMBERS        -> #0C94FF
   VARIABLE PARAMETERS     -> #E0B040

   ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/
   "semanticTokenColors": {
      "class": "#C490FF",
      "interface": "#C490FF",
      "struct": "#C490FF",
      "namespace": "#C490FF",
      "function": "#0C94FF",
      "method": "#0C94FF",
      "variable": "#90C2F8",
      "property": "#90C2F8",
      "variable.declaration": "#1EBCC4",
      "property.declaration": "#1EBCC4",
      "parameter": "#E0B040"
      // "type": "#FFF0",
      // "typeParameter""#FFF0",
   },
   "tokenColors": [
      {
         "name": "COMMENTS",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "foreground": "#407020"
         }
      },
      {
         "name": "PUNCTUATION",
         "scope": ["variable", "punctuation", "meta.brace"],
         "settings": {
            "foreground": "#A0A0A0"
         }
      },
      {
         "name": "OPERATORS",
         "scope": ["keyword.operator"],
         "settings": {
            "foreground": "#40D080"
         }
      },
      {
         "name": "STRING LITERAL",
         "scope": ["string", "punctuation.definition.string"],
         "settings": {
            "foreground": "#A0B070"
         }
      },
      {
         "name": "BOOLEAN LITERAL, UNDEFINED LITERAL, NULL LITERAL, NAN LITERAL, & UTF/ANSI Esc",
         "scope": ["constant"],
         "settings": {
            "foreground": "#EEAA33"
         }
      },
      {
         "name": "NUMERIC LITERAL",
         "scope": ["constant.numeric"],
         "settings": {
            "foreground": "#F28848",
            "fontStyle": ""
         }
      },
      {
         "name": "KEYWORDS: STORAGE DECLARATORS",
         "scope": ["storage.type", "keyword.control.import"],
         "settings": {
            "foreground": "#AA70FF"
         }
      },
      {
         "name": "KEYWORDS: CONTROL-FLOW, CONDITIONALS, MODIFIERS, etc",
         "scope": [
            "keyword.control",
            "keyword.other",
            "storage.modifier",
            "storage.type.function"
         ],
         "settings": {
            "foreground": "#AA70FF",
            "fontStyle": "italic"
         }
      },
      {
         "name": "KEYWORDS: OPERATOR EXPRESSIONS",
         "scope": [
            "keyword.operator.new",
            "keyword.operator.expression",
            "keyword.operator.type.annotation"
         ],
         "settings": {
            "foreground": "#40A0FF",
            "fontStyle": "italic"
         }
      },
      {
         "name": "VARIABLES: LANGUAGE DEFINED",
         "scope": ["variable.language", "support.variable"],
         "settings": {
            "foreground": "#1EBCC4"
         }
      },
      {
         "name": "VARIABLES: THIS (lexically-scoped object)",
         "scope": ["variable.language.this"],
         "settings": {
            "foreground": "#1EBCC4",
            "fontStyle": "italic"
         }
      },
      {
         "name": "VARIABLES: PARAMETERS",
         "scope": ["variable.parameter"],
         "settings": {
            "foreground": "#D8CE77"
         }
      },
      {
         "name": "VARIABLES: CLASS-MEMBERS & OBJECT-MEMBERS",
         "scope": [
            "variable.object.property",
            "meta.object.member",
            "meta.field.declaration",
            "meta.definition variable",
            "support.variable.property"
         ],
         "settings": {
            "foreground": "#1EBCC4"
         }
      },
      {
         "name": "TYPES & CLASSES",
         "scope": [
            "support.type",
            "support.class",
            "entity.name.class",
            "entity.other.inherited-class",
            "entity.name.type",
            "entity.name.namespace",
            "entity.name.interface",
            "entity.other.attribute",
            "entity.name.scope-resolution"
         ],
         "settings": {
            "foreground": "#C490FF"
         }
      },
      {
         "name": "FUNCTION: FUNCTION/METHOD DEFINITIONS & CALLS",
         "scope": "entity.name.function",
         "settings": {
            "foreground": "#0C94FF"
         }
      },
      {
         "name": "FUNCTION: DECLARATION",
         "scope": ["meta.definition.function entity.name.function"],
         "settings": {
            "foreground": "#0C94FF",
            "fontStyle": "bold"
         }
      },
      {
         "name": "FUNCTION: MATH SUPPORT",
         "scope": ["support.function.math"],
         "settings": {
            "foreground": "#0C94FF",
            "fontStyle": "italic"
         }
      },
      {
         "name": "PRIMITIVE TYPE ANNOTATIONS (TypeScript)",
         "scope": ["support.type.primitive"],
         "settings": {
            "foreground": "#9E5060",
            "fontStyle": "italic"
         }
      },
      {
         "name": "STRING TEMPLATE: EMBEDDED VARIABLE PUNCTUATION (ECMAScript Languages)",
         "scope": ["punctuation.definition.template-expression"],
         "settings": {
            "foreground": "#AA70FF"
         }
      },
      {
         "name": "CONSTANT: MATH-SUPPORT",
         "scope": ["support.constant.math"],
         "settings": {
            "foreground": "#90C2F8",
            "fontStyle": "italic"
         }
      },
      /*------------------------------------------*
                       JSON/JSONC
      *-------------------------------------------*/
      {
         "scope": ["punctuation.definition.block.tag", "variable.other.jsdoc"],
         "settings": {
            "foreground": "#90300E",
            "fontStyle": "bold"
         }
      },
      {
         "scope": "storage.type.class.jsdoc",
         "settings": {
            "foreground": "#007EC4"
         }
      },
      {
         "name": "JSON/JSONC: PROPERTY-KEYS",
         "scope": [
            "support.type.property-name",
            "punctuation.support.type.property-name"
         ],
         "settings": {
            "foreground": "#2Da2ff",
            "fontStyle": ""
         }
      },
      /*-----------------------------------------*
        |> REGULAR EXPRESSIONS
       *-----------------------------------------*/
      {
         "name": "REGEX",
         "scope": "string.regexp",
         "settings": {
            "foreground": "#C2C492"
         }
      },
      {
         "name": "REGEX",
         "scope": "string.regexp keyword",
         "settings": {
            "foreground": "#BBBBBB"
         }
      },
      {
         "name": "REGEX",
         "scope": "string.regexp keyword.operator",
         "settings": {
            "foreground": "#BBBBBB"
         }
      },
      {
         "name": "REGEX: QUANTIFIER",
         "scope": "string.regexp keyword.operator.quantifier",
         "settings": {
            "foreground": "#80BB80"
         }
      },
      {
         "name": "REGEX: ANCHORS & FLAGS",
         "scope": [
            "string.regexp keyword.other",
            "keyword.control.anchor.regexp"
         ],
         "settings": {
            "foreground": "#8888EE",
            "fontStyle": "bold"
         }
      },
      {
         "name": "",
         "scope": "constant.other.character-class.range.regexp",
         "settings": {
            "foreground": "#CC7777"
         }
      },
      {
         "name": "REGEX",
         "scope": "string.regexp punctuation.definition",
         "settings": {
            "foreground": "#AAAAAA"
         }
      },
      /*

      *----------------------------------------------------------------------*
                                 TOKENS
      *----------------------------------------------------------------------*/
      {
         "scope": "token.info-token",
         "settings": {
            "foreground": "#6796E6"
         }
      },
      {
         "scope": "token.warn-token",
         "settings": {
            "foreground": "#CD9731"
         }
      },
      {
         "scope": "token.error-token",
         "settings": {
            "foreground": "#F44747"
         }
      },
      {
         "scope": "token.debug-token",
         "settings": {
            "foreground": "#B267E6"
         }
      }
   ]
}
