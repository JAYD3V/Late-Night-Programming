   /*
























   *---------------------------------------------------------------------------*
                     SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*
   LITERAL BOOLEAN         -> #EEA0FF
   LITERAL NUMERIC         -> #BBAA44
   LITERAL STRINGS         -> #DDAA99

   VARIABLES               -> #90C2F8
   VARIABLE DECLARATIONS   -> #C499D0
   OBJECT-MEMBERS          -> #C499D0

   CLASS                   -> #AA88FF
   CLASS-MEMBERS           -> #C499D0

   FUNCTION-MEMBERS        -> #30A0FF
   VARIABLE PARAMETERS     -> #BBCC80

   ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/
   "semanticTokenColors": {
      "class": "#AA88FF",
      "interface": "#AA88FF",
      "struct": "#AA88FF",
      "namespace": "#AA88FF",
      "function": "#30A0FF",
      "method": "#30A0FF",
      "variable": "#BDBDBD",
      "property": "#90C2F8",
      "variable.declaration": "#C499D0",
      "property.declaration": "#C499D0",
      "parameter": "#BBCC80"
   },
   "tokenColors": [
      {
         "name": "COMMENTS",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "foreground": "#306888"
         }
      },
      {
         "name": "PUNCTUATION",
         "scope": ["punctuation", "meta.brace"],
         "settings": {
            "foreground": "#808080",
            "fontStyle": ""
         }
      },
      {
         "name": "VARIABLES",
         "scope": ["variable"],
         "settings": {
            "foreground": "#C2C2C2",
            "fontStyle": ""
         }
      },
      {
         "name": "STRINGS",
         "scope": ["string", "punctuation.definition.string"],
         "settings": {
            "foreground": "#55BBCC",
            "fontStyle": ""
         }
      },
      {
         "name": "CONSTANTS",
