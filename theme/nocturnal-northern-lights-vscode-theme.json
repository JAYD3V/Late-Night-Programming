         "settings": {
            "foreground": "#BB70AA",
            "fontStyle": "italic"
         }
      },
      {
         "name": "NUMERIC LITERALS",
         "scope": ["constant.numeric"],
         "settings": {
            "foreground": "#77CC66",
            "fontStyle": ""
         }
